-static int cls_key_off(struct dpaa2_eth_priv *priv, int prot, int field)
-{
-	int i, off = 0;
-
-	for (i = 0; i < priv->num_hash_fields; i++) {
-		if (priv->hash_fields[i].cls_prot == prot &&
-		    priv->hash_fields[i].cls_field == field)
-			return off;
-		off += priv->hash_fields[i].size;
-	}
-
-	return -1;
-}
