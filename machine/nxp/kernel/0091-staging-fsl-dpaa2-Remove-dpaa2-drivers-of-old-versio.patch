-	int err;
-
-	list_for_each(pos, &priv->port_list) {
-		port_priv = list_entry(pos, struct evb_port_priv, list);
-
-		err = _evb_port_carrier_state_sync(port_priv->netdev);
-		if (err)
-			netdev_err(port_priv->netdev,
-				   "_evb_port_carrier_state_sync err %d\n",
-				   err);
-	}
-
-	return 0;
-}
