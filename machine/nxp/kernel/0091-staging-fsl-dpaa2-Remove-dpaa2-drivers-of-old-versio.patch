-			     const struct dpaa2_fd *fd,
-			     struct napi_struct *napi __always_unused,
-			     u16 queue_id __always_unused)
-{
-	struct device *dev = priv->net_dev->dev.parent;
-	dma_addr_t addr = dpaa2_fd_get_addr(fd);
