-			dev_err(dev, "dpni_set_taildrop() failed (%d)\n", err);
-			return err;
-		}
-
-		dev_dbg(dev, "%s taildrop for Rx queue id %d tc %d\n",
-			(td->enable ? "Enabled" : "Disabled"),
