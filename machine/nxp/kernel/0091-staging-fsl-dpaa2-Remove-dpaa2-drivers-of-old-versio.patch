-	enum dpaa2_eth_swa_type type;
-	union {
-		struct {
-			struct sk_buff *skb;
-		} single;
-		struct {
-			struct sk_buff *skb;
-			struct scatterlist *scl;
-			int num_sg;
-			int num_dma_bufs;
-		} sg;
-	};
-};
