CONFIG_SYS_EEPROM_I2C_WRITE_3B_DELAY=5000
CONFIG_SYS_EEPROM_OFFSET=0
CONFIG_SYS_EEPROM_SIZE=2048
CONFIG_SYS_EEPROM_MAX_SIZE=16384
