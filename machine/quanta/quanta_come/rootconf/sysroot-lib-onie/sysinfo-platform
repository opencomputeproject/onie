# Use the contents of /sys/class/net/eth0/address

get_ethaddr()
{
    [ -r /sys/class/net/eth0/address ] && cat /sys/class/net/eth0/address
}

# Local Variables:
# mode: shell-script
# eval: (sh-set-shell "/bin/sh" t nil)
# End:
