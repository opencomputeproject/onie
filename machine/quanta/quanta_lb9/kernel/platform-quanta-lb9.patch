+			#size-cells = <0>;
+			cell-index = <0>;
+			compatible = "fsl-i2c";
+			reg = <0x3000 0x100>;
+			interrupts = <43 2>;
+			interrupt-parent = <&mpic>;
+			dfsrr;
+
+			rtc@68 {
+				compatible = "dallas,ds1339";
+				reg = <0x68>;
+			};
+
+			board_eeprom@53 {
+				compatible = "at,24c02";
+				reg = <0x53>;
+				label = "board_eeprom";
+			};
+
+			spd@57 {
+				compatible = "";
+				reg = <0x57>;
+			};
+
+			mux@70 {
+				compatible = "ti,pca9548";
+				reg = <0x70>;
+				#address-cells = <1>;
+				#size-cells = <0>;
+
+				// 10G SFP+ 0
+				i2c@0 {
+					#address-cells = <1>;
+					#size-cells = <0>;
+					reg = <0>;
+					sfp_eeprom@50 {
+						compatible = "at,24c02";
+						reg = <0x50>;
+					};
+					sfp_eeprom@51 {
+						compatible = "at,24c02";
+						reg = <0x51>;
+					};
+				};
+				// 10G SFP+ 1
+				i2c@1 {
+					#address-cells = <1>;
+					#size-cells = <0>;
+					reg = <1>;
+					sfp_eeprom@50 {
+						compatible = "at,24c02";
+						reg = <0x50>;
+					};
+					sfp_eeprom@51 {
+						compatible = "at,24c02";
+						reg = <0x51>;
+					};
+				};
+				// 10G SFP+ 2
+				i2c@2 {
+					#address-cells = <1>;
+					#size-cells = <0>;
+					reg = <2>;
+					sfp_eeprom@50 {
