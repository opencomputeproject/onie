+/ {
+	model = "quanta,lb9";
+	compatible = "quanta,lb9";
+	reset-gpio = <&cpm_pio_a 24 1>;
+	#address-cells = <1>;
+	#size-cells = <1>;
+
+	aliases {
+		ethernet0 = &enet0;
+		ethernet1 = &enet1;
+		serial0 = &serial0;
+		serial1 = &serial1;
+		pci0 = &pci0;
+		pci1 = &pci1;
