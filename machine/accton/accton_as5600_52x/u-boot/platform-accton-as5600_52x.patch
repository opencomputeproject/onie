+    diag_printf("END Address: 0x%08lX\n", endAddr);
+	last_percent = 0;
+	timer_change = 0;
+	timer_keep = get_timer(0);
+    diag_printf("Data Bus Test...\n");
+
+    if(diagMemDataBusTest(startAddr, endAddr) == 0)
+    {
+        diag_printf("\r%08lx~%08lx 100%% OK!\n",startAddr,endAddr);
+    }
+    else
