+#else
+			if (unlikely(net_ratelimit())) {
+				dev_warn(tx_ring->dev,
+				 "partial checksum but l4 proto=%x!\n",
+				 l4_hdr);
+			}
+#endif /* HAVE_ENCAP_TSO_OFFLOAD */
+			break;
+		}
