+		errStat = __raw_readl(AXIIC_DS_0_IDM_ERROR_LOG_STATUS_VA);
+//		printk("%s: %d, %d\n", __FUNCTION__, __LINE__, errStat);
+	}
+//	printk("%s: %d exit\n", __FUNCTION__, __LINE__);
+	return IRQ_HANDLED;
