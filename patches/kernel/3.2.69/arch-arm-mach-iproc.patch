+	.irq        = IRQ_IPROC_##name,					\
+	.uartclk    = CONFIG_CYGNUS_EMULATION_SCLK, /*.uartclk    = 62500000,*/						\
+	.regshift   = 2,						\
+	.iotype     = UPIO_MEM32,						\
+	.type       = PORT_16550A,					\
+	.flags      = UPF_FIXED_TYPE | UPF_SHARE_IRQ,			\
+	.private_data = (void __iomem *)((IPROC_##name##_VA) + 0x00),	\
+}
+#else /* ! CONFIG_MACH_CYGNUS */
